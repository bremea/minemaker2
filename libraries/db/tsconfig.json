{
	"compilerOptions": {
		"baseUrl": "./",
		"paths": {
			"$src": ["src"],
			"$src/*": ["src/*"],
			"$types/*": ["src/types/*"]
		},
		"target": "ES2021",
		"module": "ES2022",
		"moduleResolution": "node",
		"rootDirs": [".."],
		"types": ["bun-types"],
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"isolatedModules": true,
		"skipLibCheck": true,
		"esModuleInterop": true
	}
}
